{
  "name": "mintel.me",
  "description": "Personal website of Marc Mintel",
  "version": "1.0.0",
  "private": true,
  "author": "Marc Mintel <marc@mintel.me> (mmintel)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "react",
    "contentful",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "start:coverage": "nyc --silent next start",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.{js,jsx,ts,tsx}\" --write",
    "test": "LOG_LEVEL=error jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test:coverage --ci",
    "pree2e": "node ./scripts/e2e.js",
    "e2e": "start-test start:coverage 3000 cy:open",
    "e2e:ci": "start-test start:coverage 3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "precopy:reports": "mkdir reports || true",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precoverage": "npm run copy:reports && mkdir .nyc_output || true && npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "coverage": "npx nyc report --reporter lcov --reporter text --report-dir coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@react-hook/mouse-position": "^4.1.0",
    "@react-hook/size": "^2.1.1",
    "@rooks/use-debounce": "^3.6.0",
    "@rooks/use-mouse": "^3.6.0",
    "@rooks/use-throttle": "^3.6.0",
    "@sentry/webpack-plugin": "^1.13.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/classnames": "^2.2.11",
    "@zeit/next-source-maps": "0.0.3",
    "chalk": "^4.1.0",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "dot-object": "^2.1.3",
    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "framer-motion": "^2.9.4",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.19",
    "next": "^10.0.0",
    "next-images": "^1.6.2",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-markdown": "^6.0.2",
    "react-parallax-mousemove": "^1.1.0",
    "react-test-renderer": "^16.14.0",
    "react-tippy": "^1.4.0",
    "shallow-equal-object": "^1.1.1",
    "tailwindcss": "^1.9.6",
    "tslog": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@cypress/code-coverage": "^3.8.3",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/chalk": "^2.2.0",
    "@types/dot-object": "^2.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cypress": "^5.5.0",
    "cypress-html-validate": "^1.2.5",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-validate": "^3.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.2",
    "nyc": "^15.1.0",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.1",
    "start-server-and-test": "^1.11.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "exclude": [
      ".next",
      "pages/api/__coverage__.js"
    ]
  },
  "engines": {
    "node": "^12.18.3",
    "yarn": "^1.22.4"
  }
}
